{
   "name": "Tank Control",
   "version": "0.1",
   "manifest_version": 2,
   "description": "Control a tank using UDP yo", 
   "content_scripts": [
     {
       "matches": [
          "<all_urls>"
        ], 
        "js": [
          "content.js", "jquery-3.5.1.js", "background.js"
        ]
     }
   ],
   "icons": {
       "128":"tankControl.png",
       "48":"tank48.ico"
   },
   "sockets": {
      "udp": {
        "send": ["*"],
        "bind": ["*"]
      }
   },
   "permissions": [
    "alarms"
  ]
}
